{
  "name": "react-tiny-popover",
  "version": "7.1.0",
  "description": "A simple and highly customizable popover solid-js higher order component with no other dependencies! Typescript friendly.",
  "keywords": [
    "solid",
    "popover",
    "solid-popover",
    "pop",
    "out",
    "modal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkatz/react-tiny-popover.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "AmirAhmadzadeh",
  "main": "dist/Popover.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p . && yarn copy-types",
    "clean": "rm -rf dist/",
    "copy-types": "cp src/index.d.ts dist/index.d.ts",
    "start-demo": "cd docs && yarn start",
    "watch": "tsc-watch -p . --onSuccess 'yarn copy-types'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.8.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "tsc-watch": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "dayjs": "^1.11.3",
    "socket.io-client": "^4.5.1",
    "solid-app-router": "^0.3.3",
    "solid-js": "^1.4.2"
  }
}
