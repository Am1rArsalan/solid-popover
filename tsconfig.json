{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "sourceMap": true,
    "inlineSources": true,
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsxImportSource": "solid-js", 
    "jsx": "preserve",
    "pretty": true
  },
  "include": ["src"], 
  "exclude": ["node_modules", "**/__tests__/*"]
}
